<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="es.tfg.modelo.Token" table="TOKEN">
        <id name="token" column="TOKEN"></id>
        <property name="caducidad" type="timestamp" column="F_CADUCIDAD" />
        <property name="idUsuario" type="string" column="IDUSUARIO" />
    </class>
</hibernate-mapping>
