<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="es.tfg.modelo.Aporte" table="APORTE">
    <id column="ID" name="id">
      <generator class="identity"/>
    </id>
    <property column="TITULO" name="titulo" type="string"/>
    <property column="DESCRIPCION" name="descripcion" type="string"/>
    <property column="F_CREACION" name="f_creacion" type="timestamp"/>
    <property column="URLIMAGEN" name="urlImagen" type="string"/>
    <property column="PROPIETARIO" name="propietario" type="string"/>
    <property column="TIPO" name="tipo" type="string"/>
  </class>
</hibernate-mapping>
